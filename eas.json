{
  "cli": {
    "version": ">= 16.13.1",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "android": {
        "buildType": "apk"
      },
      "environment": "development"
    },
    "preview": {
      "distribution": "internal",
      "channel": "preview",
      "android": {
        "buildType": "apk"
      },
      "environment": "preview"
    },
    "production": {
      "distribution": "store",
      "channel": "stable",
      "android": {
        "buildType": "app-bundle",
        "image": "latest",
        "autoIncrement": "version"
      },
      "environment": "production"
    }
  }
}
